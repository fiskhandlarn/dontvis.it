  "description": "dontvis.it, the idiot circumventor tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Oskar Joelson",
      "email": "oskar@joelson.org"
    }
  ],
  "homepage": "https://github.com/fiskhandlarn/dontvis.it#readme",
